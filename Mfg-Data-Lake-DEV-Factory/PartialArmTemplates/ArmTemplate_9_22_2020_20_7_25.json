					},
					{
						"name": "merge_FDC_results",
						"type": "DatabricksNotebook",
						"dependsOn": [
							{
								"activity": "detect_anomaly_ASH",
								"dependencyConditions": [
									"Succeeded"
								]
							},
							{
								"activity": "detect_anomaly_ETH",
								"dependencyConditions": [
									"Succeeded"
								]
							},
							{
								"activity": "detect_anomaly_CED",
								"dependencyConditions": [
									"Succeeded"
